<font color="#838B8B"> <h1> Project Page </h1> </font>
<br>
<h3><b> Title: </b> <%= @project.project_name %></h3>
<p> <b> Description: </b> <%= @project.description %> </p>

<br>

<h3> User who proposed the project</h3>
<p> 
  <% @owner.each do |owner| %> 
  <%= User.find_by(id: owner.user_id).name %>
  <%= User.find_by(id: owner.user_id).last_name %>
  <br>
  <br>
  <h3><b>  </b></h3><br> <%= @project.category %>
</p>

<% end %>
