<h1>Edit User</h1>
<%= link_to "Projects", projects_path %> | <%= link_to "Users", users_path %> | <%= link_to "New Project", new_project_path %> | <%= link_to "New User", new_user_path %> 

<p> ------------------------------------------------- </p>
<%= form_for @user do |form| %>
  <div class="form-group">
    <%= form.label :name %>
    <%= form.text_field :name, class: "form-control" %>
    <%= form.label :last_name %>
    <%= form.text_field :last_name, class: "form-control" %>
    <%= form.label :email %>
    <%= form.text_field :email, class: "form-control" %>
    <%= form.label :password %>
    <%= form.text_field :password, class: "form-control" %>
    <%= form.label :gender %>
    <%= form.text_field :gender, class: "form-control" %>
    <%= form.label :city %>
    <%= form.text_field :city, class: "form-control" %>
    <%= form.label :state %>
    <%= form.text_field :state, class: "form-control" %>
  </div>
  <%= form.button 'Update Project', class: "btn btn-success" %>
  <% end %>