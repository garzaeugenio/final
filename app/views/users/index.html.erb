<h1>All Users</h1>


<% @users.each do |user| %>
  <li> 
    <p> <%= link_to user.name, user_path(user) %> <%= link_to user.last_name, user_path(user) %> 

              <% if current_user %>
                  <font size="1"><%= link_to "Edit", edit_user_path(user) %> <%= link_to('Delete', user_path(user), {:onclick => "return confirm('Are you sure')", method: :delete }) %>
              <% end %>

  
      
 </font>

   </font>
    </p> 
  </li>
<% end %>